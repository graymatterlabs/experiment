{
    "name": "graymatterlabs/experiment",
    "description": "",
    "homepage": "https://github.com/graymatterlabs/experiment",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Colson",
            "email": "ryan@graymatterlabs.co"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "phpunit/phpunit": "^9.5",
        "psr/simple-cache": "^3.0"
    },
    "suggest": {
        "psr/simple-cache": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GrayMatterLabs\\Experiment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrayMatterLabs\\Experiment\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-text",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
